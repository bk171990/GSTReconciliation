    <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div id="login-wrapper">
                    <header>
                        <div class="brand">
                            <a href="/" class="logo">
                                <i class="icon-layers"></i>
                                <span>GST</span>APP</a>
                        </div>
                    </header>
                <div class="panel panel-primary">
                    <div class="panel-heading">
                          <h3 class="panel-title">     
                           Sign Up
                        </h3>  
                    </div>
                    <div class="panel-body">
                    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                    <%= devise_error_messages! %>
                        <p >Already a member? <a href="pages-login.html"><strong>Sign In</strong></a></p>
                        <form role="form">
                            <div class="form-group">
                                <label for="exampleInputPassword1">Email</label>
                                <%= f.email_field :email, autofocus: true,input_type: "email", class: "form-control", id: "exampleInputEmail1" ,placeholder: "Enter your name" %>
                            </div>
                             <div class="form-group">
                                <label for="exampleInputPassword1">Password</label>
                                <%= f.password_field :password, autocomplete: "off",input_type: "password", class: "form-control", id: "exampleInputPassword1", placeholder: "Password" %>
                            </div>
                              <div class="form-group">
                                <label for="exampleInputPassword1">Retype Password</label>
                                <%= f.password_field :password_confirmation, autocomplete: "off", input_type: "password" ,class: "form-control", id: "exampleInputPassword1", placeholder: "Retype your password" %>
                            </div>
                       
                           <%= f.submit "Sign up", class: "btn btn-primary btn-block" %>
                        </form>
                        <%end%>
                     <%= render "devise/shared/links" %>
                    </div>
                </div>
                </div>
            </div>
        </div>