  <div class="pageheader">
                <h1>Form Validation</h1>
                <div class="breadcrumb-wrapper hidden-xs">
                    <span class="label">You are here:</span>
                    <ol class="breadcrumb">
                        <li><a href="/">Dashboard</a>
                        </li>
                        <li>Forms</li>
                        <li class="active">Form Validation</li>
                    </ol>
                </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">Form Validations</h3>
                                <div class="actions pull-right">
                                 <i class="fa fa-expand"></i>
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>
                            <div class="panel-body">
                            <%= form_for @general_setting,method: :patch do |f| %>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">First Name</label>
                                        <div class="col-sm-12">
                                            
                                            <%= f.text_field :first_name,class: "form-control",placeholder: "First Name."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Last Name</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :last_name, class: "form-control" ,placeholder: "Last Name."%>
                                        </div>
                                    </div>
                                     <div class="form-group">
                                        <label class="col-sm-3 control-label">Contact No</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :contact_no, class: "form-control", placeholder: "Conatact No."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Email</label>
                                        <div class="col-sm-12">
                                             <%= f.text_field :email, class: "form-control" ,placeholder: "Email."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Business Description</label>
                                        <div class="col-sm-12">
                                             <%= f.text_field :business_description, class: "form-control", name: "input2", id: "input2", required: "" ,placeholder: "Description."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Gst No</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :gst_no, class: "form-control" ,placeholder: "No."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Pan Number</label>
                                        <div class="col-sm-12">
                                            <%= f.text_field :pan, class: "form-control",placeholder: "Pan Number."%>
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Name Of Service</label>
                                        <div class="col-sm-12">
                                             <%= f.text_field :name_of_service, class: "form-control",placeholder: "Name."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Service Account Code</label>
                                        <div class="col-sm-12">
                                           <%= f.text_field :service_acc_code, class: "form-control" ,placeholder: "Serv Code."%>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label">Rate Of Tax</label>
                                        <div class="col-sm-12">
                                           <%= f.text_field :rate_of_tax, class: "form-control" ,placeholder: "Tax Rate."%>
                                        </div>
                                    </div>
                             
                                    <div class="form-group">
                                        <div class="col-sm-offset-3 col-sm-12">
                                            <button type="submit" class="btn btn-primary">Update</button>
                                        </div>
                                    </div>
                                 
                                <%end%>
                            </div>
                        </div>
                    </div>
                </div>
        </section>

       