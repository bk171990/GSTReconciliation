
                <div class="row">
                    <div class="col-md-12">

                        <div class="panel panel-default">
                          <div class="panel-heading">
                            <h3 class="panel-title">View All Parties</h3>
                            <div class="actions pull-right">
                             <i class="fa fa-expand"></i>
                                <i class="fa fa-chevron-down"></i>
                                <i class="fa fa-times"></i>
                            </div>
                          </div>
                          <div class="panel-body">
                          <%if current_user.role == 'Party'%>
                          <%else%>
                             <%= link_to 'New Party', new_party_path, class: 'btn btn-link' %>
                          <%end%>
                            <div class="table-responsive">
                              <table class="table table-bordered table-striped">
                                <thead>
                                  <tr>
                                    <th>Party name</th>
                                    
                                    <th>State Name</th>
                                    <th>State Code</th>
                                    <th>CA Name</th>
                                    <th>CA Contact</th>
                                    <th>Composite</th>
                                    <th>Regular</th>
                                    <th colspan="3"></th>
                                  </tr>
                                </thead>

                                <tbody>
                                <%if current_user.role == 'Party'%>
                                
                                  <tr>
                                      <td><%= @parties.party_name %></td>
                                      <td><%= @parties.state_name %></td>
                                      <td><%= @parties.state_code %></td>
                                      <td><%= @parties.ca_name %></td>
                                      <td><%= @parties.ca_contact %></td>
                                      <td><%= @parties.composite %></td>
                                      <td><%= @parties.regular %></td>
                                  </tr>
                                <%else%>  
                                  <% @parties.each do |party| %>
                                    <tr>
                                      <td><%= party.party_name %></td>
                                      <td><%= party.state_name %></td>
                                      <td><%= party.state_code %></td>
                                      <td><%= party.ca_name %></td>
                                      <td><%= party.ca_contact %></td>
                                      <td><%= party.composite %></td>
                                      <td><%= party.regular %></td>
                                      <td><%= %></td>
                                      <td><%= link_to 'Show', party %></td>
                                      <td><%= link_to 'Edit', edit_party_path(party) %></td>
                                      <td><%= link_to 'Destroy', party, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                                    </tr>
                                  <% end %>
                                  <% end %>
  </tbody>
</table>
</div>
</div>
</div>
</div>
             </div>
<br>
