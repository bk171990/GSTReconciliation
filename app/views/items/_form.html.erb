  <div class="pageheader">
                <h1>Create New Item</h1>
                <div class="breadcrumb-wrapper hidden-xs">
                    <span class="label">You are here:</span>
                    <ol class="breadcrumb">
                        <li><a href="/">Dashboard</a>
                        </li>
                        <li>Forms</li>
                        <li class="active">New Item</li>
                    </ol>
                </div>
            </div>
            <section id="main-content" class="animated fadeInUp">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">
                                <h3 class="panel-title">New Item</h3>
                                <div class="actions pull-right">
                                 <i class="fa fa-expand"></i>
                                    <i class="fa fa-chevron-down"></i>
                                    <i class="fa fa-times"></i>
                                </div>
                            </div>
                            <div class="panel-body">
                            <%= form_with(model: item, local: true) do |f| %>
                             <% if item.errors.any? %>
                                <div id="error_explanation">
                                  <h2><%= pluralize(item.errors.count, "error") %> prohibited this item from being saved:</h2>

                                  <ul>
                                  <% item.errors.full_messages.each do |message| %>
                                    <li><%= message %></li>
                                  <% end %>
                                  </ul>
                                </div>
                              <% end %>
                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-20 control-label">Description Of Goods And Services</label>
                                        <%= f.text_field :dogns, id: :item_dogns,class: "form-control",placeholder: "Descriptn of good services" %>
                                      </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-6 control-label">HSN No</label>
                                          <%= f.text_field :item_hsn_no, id: :item_item_hsn_no, class: "form-control" ,placeholder: "HSN No." %>
                                        </div>
                                    </div>

                                     <div class="form-group">
                                       <div class="col-sm-3">
                                        <label class="col-sm-6 control-label">UOM</label>
                                          <%= f.text_field :uom, id: :item_uom, class: "form-control", placeholder: "UOM No." %>
                                       </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-6 control-label">Quantity</label>
                                          <%= f.text_field :qty, id: :item_qty, class: "form-control" ,placeholder: "Qty" %>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-8 control-label">Rate</label>
                                          <%= f.text_field :rate, id: :item_rate, class: "form-control" ,placeholder: "Rate" %>
                                      </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-8 control-label">CGST</label>
                                          <%= f.text_field :cgst, id: :item_cgst, class: "form-control" ,placeholder: "CGST" %>
                                      </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-8 control-label">SGST</label>
                                          <%= f.text_field :sgst, id: :item_sgst, class: "form-control" ,placeholder: "CGST" %>
                                      </div>
                                    </div>

                                    <div class="form-group">
                                      <div class="col-sm-3">
                                        <label class="col-sm-6 control-label">Taxable Value</label>
                                          <%= f.text_field :taxable_value, id: :item_taxable_value, class: "form-control" ,placeholder: "Value" %>
                                      </div>
                                    </div>

                                    <br><br><br><br><br><br>
                             
                                     <div class="panel-body">
                                        <div class="col-sm-offset-3 col-sm-12">
                                        <%= f.submit class: "btn btn-success"%>
                                        <%= link_to 'Back', items_path,class: "btn btn-primary" %>
                                        </div>
                                    </div>
                                 
                                <%end%>
                            </div>
                        </div>
                    </div>
                </div>
        </section>